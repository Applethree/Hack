#include <iostream>
#include <algorithm>
using namespace std;
int a[10000000];

int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
    int n;
    cin>>n;
    int val=0;
    for(int i=0;i<n;i++)
    {
       cin>>a[i];
       val^=a[i];
    }
    int res=0;
    for(int i=0;i<n;i++)
    {
       if((val^a[i])<a[i])
       res+=1;
    }
    cout<<res;  
    return 0;
}
